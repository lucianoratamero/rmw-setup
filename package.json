{
  "name": "rwm-setup",
  "version": "0.0.1",
  "description": "react-webpack-mocha basic setup",
  "main": "src/app.js",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "mocha": "^4.1.0",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-dotenv-plugin": "^2.0.2",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint src/*.js",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "test": "./node_modules/.bin/mocha --require babel-core/register ./tests/**/*.spec.js",
    "tdd": "./node_modules/.bin/mocha --require babel-core/register ./tests/**/*.spec.js --watch",
    "debug": "./node_modules/.bin/mocha --require babel-core/register --reporter spec --inspect-brk ./tests/**/*.spec.js",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --colors --progress --config ./webpack/prod.config.js",
    "start": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --inline --open --config ./webpack/dev.config.js",
    "deploy": "npm run build && git subtree push --prefix build origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucianoratamero/rmw-setup.git"
  },
  "keywords": [
    "react",
    "webpack",
    "mocha",
    "setup"
  ],
  "author": "Luciano Ratamero <luciano@ratamero.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucianoratamero/rmw-setup/issues"
  },
  "homepage": "https://github.com/lucianoratamero/rmw-setup#readme"
}
